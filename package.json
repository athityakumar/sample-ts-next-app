{
  "name": "sample-ts-next-app",
  "homepage": "./",
  "private": true,
  "scripts": {
    "dev": "rm -rf next.config.js && cp next_config/local_dev.config.js next.config.js && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "static": "next build && next export",
    "local_static": "rm -rf next.config.js && cp next_config/local_static.config.js next.config.js && yarn static",
    "gh_static": "rm -rf next.config.js && cp next_config/gh_static.config.js next.config.js && yarn static",
    "gh-pages": "yarn gh_static && cp .nojekyll out/.nojekyll && gh-pages -d out -m 'feat: pushes gh-pages docs' -t true"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/react": "^2.0.0",
    "@mui/icons-material": "^5.3.1",
    "@mui/material": "^5.4.0",
    "@next/mdx": "^12.0.8",
    "autoprefixer": "^10.4.2",
    "daisyui": "^1.22.0",
    "gray-matter": "^4.0.3",
    "next": "12.0.7",
    "next-themes": "^0.0.15",
    "postcss": "^8.4.5",
    "react": "17.0.2",
    "react-dark-mode-toggle": "^0.2.0",
    "react-dom": "17.0.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "tailwindcss": "^3.0.12"
  },
  "devDependencies": {
    "@types/node": "17.0.6",
    "@types/react": "17.0.38",
    "eslint": "8.6.0",
    "eslint-config-next": "12.0.7",
    "faker": "^6.6.6",
    "gh-pages": "^3.2.3",
    "typescript": "4.5.4"
  }
}
